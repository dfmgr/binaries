  printf_help "Usage: explainshell ls -ltrsa"
  printf_help "list directory contents"
  printf_help "-l   use a long listing format"
  printf_help "-t   sort by modification time, newest first"
  printf_help "-r   reverse order while sorting"
  printf_help "-s   print the allocated size of each file, in blocks"
  printf_help "-a   do not ignore entries starting with ."
