  printf_help "The Default action is to clone/pull"
  printf_help "Usage:"
  printf_help "--single username reponame"
  printf_help "--org username"
  printf_help "--user usename"
  printf_help "--push to push the repos or"
  printf_help "--clone to clone/update local repos"
  printf_help "--help --org --user --push"
  printf_help "--clone --pull --create"
  printf_help "--delete --gist"
