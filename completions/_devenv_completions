# @Copyright     : Copyright: (c) 2021 Jason Hempstead, CasjaysDev
# @Created       : Friday, Feb 05, 2021 11:22 EST
# @File          : devenv
# @Description   : autocomplete for devenv
# @TODO          :
# @Other         :
# @Resource      :
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
_devenv() {
  local CASJAYSDEVDIR="${CASJAYSDEVDIR:-/usr/local/share/CasjaysDev/scripts}"
  local cur prev words cword
  local ARRAY="$(<$CASJAYSDEVDIR/scripts/helpers/devenv/array)"
  local LIST="$(<$CASJAYSDEVDIR/scripts/helpers/devenv/list)"
  local START="$(<$CASJAYSDEVDIR/scripts/helpers/devenv/start)"

  _init_completion || return

  case $prev in
  --start)
    COMPREPLY=($(compgen -W '${START}' -- "$cur"))
    return
    ;;

  esac

  COMPREPLY=($(compgen -W '${ARRAY} ${LIST}' -- "$cur"))
} &&
  # - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
  # enable completions
  complete -F _devenv -o default emj &&
  complete -F _devenv -o default devenv
