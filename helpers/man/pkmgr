  printf_help '                A Package Manager for most systems'
  printf_help 'pkmgr dotfiles      - install required packages     | IE: pkmgr dotfiles git'
  printf_help 'pkmgr list          - install from a file           | IE: pkmgr list [full path to list]'
  printf_help 'pkmgr curl          - install from a url            | IE: pkmgr curl debian min [ See '$REPO' for details]'
  printf_help 'pkmgr script        - run a script                  | IE: pkmgr script [ ./pathToScript or http://fullurltoscript ]'
  printf_help 'pkmgr search        - find a package                | IE: pkmgr search myapp'
  printf_help 'pkmgr install       - install a package             | IE: pkmgr install myapp'
  printf_help 'pkmgr remove        - remove a package              | IE: pkmgr remove myapp'
  printf_help 'pkmgr update        - update all packages           | IE: pkmgr update'
  printf_help 'pkmgr clean         - Removes cached files          | IE: pkmgr clean'
  printf_help 'pkmgr makecache     - update package lists          | IE: pkmgr makecache'
  printf_help 'pkmgr silent        - silently install package      | IE: pkmgr silent myapp'
  printf_help 'pkmgr silent-remove - silently remove package       | IE: pkmgr silent-remove myapp'
  printf_help 'pkmgr cron add      - schedule daily updates        | IE: pkmgr cron add'
  printf_help 'pkmgr aliases       - lists the alias for bash      | IE: pkmgr alias'
  printf_help 'pkmgr pip           - install a python package      | IE: pkmgr pip requests'
  printf_help 'pkmgr cpan          - install a perl module         | IE: pkmgr cpan Bundle::Apache'
  printf_help 'pkmgr gem           - install a gem module          | IE: pkmgr gem jekyll'
  printf_help 'pkmgr npm           - install a nodejs module       | IE: pkmgr npm express'
  printf_help 'pkmgr cmd --help    - show help for the command     | IE: pkmgr aliases --help'
