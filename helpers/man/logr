printf_help "A simple logging utility"
printf_help "Usage       : log mycommand"
printf_help "--clean     : remove old logs"
printf_help "--show      : will show all output"
printf_help "--silent    : no output"
printf_help "--err       : show only error"
printf_help "--noerr     : hide error mesaages"
printf_help "--list      : list all logs"
printf_help "--open      : open logdir"
