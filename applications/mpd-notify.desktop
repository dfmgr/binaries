#!/usr/bin/env xdg-open

[Desktop Entry]
Version=1.1
Type=Application
Name=MPD Notify
GenericName=MPDNotify
Comment=Get MPD Notifications.
Icon=multimedia
Exec=myterminal -e 'mpd-notify start'
Path=~
Terminal=false
Actions=
Categories=Audio;Player;System;GTK;X-XFCE;GNOME;MyScripts
Keywords=mpd;music;player;my;
StartupNotify=true
